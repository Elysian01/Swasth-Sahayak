{
  "expo": {
    "plugins": [
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Swasth-Sahayak-Health-Worker-App to use Face ID."
        }
      ]
    ],
    "name": "Swasth-Sahayak-Health-Worker-App",
    "slug": "swath-sahayak-feildworker",
    "version": "1.0.0",
    "icon": "./assets/icon_with_bg.png",
    "splash": {
      "image": "./assets/icon.png",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.elysian01.SwasthSahayakHealthWorkerApp",
      "infoPlist": {
        "NSFaceIDUsageDescription": "Allow Swasth-Sahayak-Health-Worker-App to use Face ID."
      }
    },
    "android": {
      "package": "com.elysian01.SwasthSahayakHealthWorkerApp",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "extra": {
      "eas": {
        "projectId": "ef5f02a4-62d8-4af2-872b-f2bc82c8c09c"
      }
    },
    "owner": "farmanahmed888"
  }
}
